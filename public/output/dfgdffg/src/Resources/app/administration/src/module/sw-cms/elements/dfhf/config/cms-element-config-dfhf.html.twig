{% block dfhf_config %}
    <sw-container>
            <sw-text-editor
              v-model="element.config.fh.value"
              :label="$tc('sw-cms.elements.image.config.fhLabel.fh')"
              :allow-inline-data-mapping="true"
              sanitize-input
              @input="emitChanges"
              @blur="emitChanges"
        />
        <sw-text-field
              v-model="element.config.dhf.value"
              :label="$tc('sw-cms.elements.image.config.dhfLabel.dhf')"
              :placeholder="$tc('sw-cms.elements.image.config.placeholder.enterMinHeight')"
              @input="emitChanges"
        />
        </sw-text-field>
    </sw-container>
{% endblock %}